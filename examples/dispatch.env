# Logging level
RUST_LOG=info

# Discord bot token
BOT_TOKEN=

# Sharding information
SHARDS_START=0
SHARDS_END=1
SHARDS_TOTAL=2
SHARDS_CONCURRENCY=1

# Gateway API version
API_VERSION=6

# Resume after a restart
RESUME=true

# Identify payload
INTENTS=4739
LARGE_THRESHOLD=50
STATUS=online
ACTIVITY_TYPE=playing
ACTIVITY_NAME="https://wyvor.xyz"

# Discord channel logs
LOG_CHANNEL=

# RabbitMQ address
RABBIT_HOST=127.0.0.1
RABBIT_PORT=5672

# Redis address
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# Prometheus address
PROMETHEUS_HOST=127.0.0.1
PROMETHEUS_PORT=8005
